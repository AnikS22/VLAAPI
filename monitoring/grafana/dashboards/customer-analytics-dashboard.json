{
  "dashboard": {
    "id": null,
    "uid": "vla-customer-analytics",
    "title": "VLA API Customer Analytics Dashboard",
    "tags": ["vla", "customer", "analytics"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "5m",
    "time": {
      "from": "now-30d",
      "to": "now"
    },
    "templating": {
      "list": [
        {
          "name": "customer_id",
          "type": "query",
          "query": "label_values(vla_inference_requests_total, customer_id)",
          "refresh": 1,
          "multi": false,
          "includeAll": false
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Usage Patterns by Robot Type",
        "type": "graph",
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum by(robot_type) (increase(vla_inference_requests_total{customer_id=\"$customer_id\"}[1d]))",
            "legendFormat": "{{robot_type}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "drawStyle": "bars",
              "fillOpacity": 60
            }
          }
        },
        "options": {
          "legend": {"displayMode": "table", "placement": "right", "values": ["value", "percent"]}
        }
      },
      {
        "id": 2,
        "title": "Instruction Categories Distribution",
        "type": "piechart",
        "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum by(instruction_category) (increase(vla_inference_requests_total{customer_id=\"$customer_id\"}[30d]))",
            "legendFormat": "{{instruction_category}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0
          }
        },
        "options": {
          "pieType": "donut",
          "legend": {"displayMode": "table", "placement": "right", "values": ["value", "percent"]}
        }
      },
      {
        "id": 3,
        "title": "Success Rate by Robot",
        "type": "bargauge",
        "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum by(robot_type) (increase(vla_inference_requests_total{customer_id=\"$customer_id\",status=\"success\"}[30d])) / sum by(robot_type) (increase(vla_inference_requests_total{customer_id=\"$customer_id\"}[30d])) * 100",
            "legendFormat": "{{robot_type}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "decimals": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 90, "color": "yellow"},
                {"value": 95, "color": "green"}
              ]
            }
          }
        },
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient"
        }
      },
      {
        "id": 4,
        "title": "Latency by Customer (p95)",
        "type": "graph",
        "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum by(le) (rate(vla_inference_duration_seconds_bucket{customer_id=\"$customer_id\"}[5m])))",
            "legendFormat": "p95 Latency",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "decimals": 3,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 1, "color": "yellow"},
                {"value": 2, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Feedback Rate",
        "type": "graph",
        "gridPos": {"x": 0, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum by(feedback_type) (increase(vla_feedback_total{customer_id=\"$customer_id\"}[1d]))",
            "legendFormat": "{{feedback_type}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "options": {
          "legend": {"displayMode": "list", "placement": "bottom"}
        }
      },
      {
        "id": 6,
        "title": "Peak Usage Hours",
        "type": "heatmap",
        "gridPos": {"x": 12, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum by(hour) (increase(vla_inference_requests_total{customer_id=\"$customer_id\"}[1h]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 7,
        "title": "Total Requests (30d)",
        "type": "stat",
        "gridPos": {"x": 0, "y": 24, "w": 6, "h": 6},
        "targets": [
          {
            "expr": "sum(increase(vla_inference_requests_total{customer_id=\"$customer_id\"}[30d]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "blue"}
              ]
            }
          }
        },
        "options": {
          "textMode": "value_and_name",
          "graphMode": "area",
          "colorMode": "background"
        }
      },
      {
        "id": 8,
        "title": "Average Daily Requests",
        "type": "stat",
        "gridPos": {"x": 6, "y": 24, "w": 6, "h": 6},
        "targets": [
          {
            "expr": "avg_over_time(sum(increase(vla_inference_requests_total{customer_id=\"$customer_id\"}[1d]))[30d:1d])",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"}
              ]
            }
          }
        },
        "options": {
          "textMode": "value_and_name",
          "graphMode": "area",
          "colorMode": "background"
        }
      },
      {
        "id": 9,
        "title": "Quota Utilization",
        "type": "gauge",
        "gridPos": {"x": 12, "y": 24, "w": 6, "h": 6},
        "targets": [
          {
            "expr": "vla_quota_used{customer_id=\"$customer_id\"} / vla_quota_limit{customer_id=\"$customer_id\"} * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "decimals": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 75, "color": "yellow"},
                {"value": 90, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 10,
        "title": "Error Rate",
        "type": "stat",
        "gridPos": {"x": 18, "y": 24, "w": 6, "h": 6},
        "targets": [
          {
            "expr": "sum(increase(vla_inference_requests_total{customer_id=\"$customer_id\",status=\"error\"}[30d])) / sum(increase(vla_inference_requests_total{customer_id=\"$customer_id\"}[30d])) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 2,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 1, "color": "yellow"},
                {"value": 5, "color": "red"}
              ]
            }
          }
        },
        "options": {
          "textMode": "value_and_name",
          "graphMode": "none",
          "colorMode": "background"
        }
      }
    ]
  }
}
